package me.max.dao;

import java.sql.Connection;
import java.util.ArrayList;

import me.max.models.Request;

public interface RequestDAO {
	public Request createRequest(Connection con, int uId, double amount, String rFor);
	public ArrayList<Request> getAllRequests(Connection con);
	public  ArrayList<Request> getUserRequests(Connection con, int uId);
	public boolean updateRequest(Connection con, int rId, int status);
}
